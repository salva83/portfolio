{
  "name": "Cypress_Cucumber_Typescript",
  "description": "Cypress + Cucumber + Typescript",
  "version": "1.0.0",
  "main": "",
  "author": "Salvador <salvador.github@gmail.com>",
  "license": "MIT",
  "scripts": {
    "cypress:open": "cypress open",
    "precypress:run": "yarn install && export CYPRESS_ENVIRONMENT=$CYPRESS_ENVIRONMENT",
    "cypress:run": "scripts/load_env_var.sh && cypress run",
    "postcypress:run": "yarn report:multiple",
    "report:multiple": "npx ts-node ./cypress/report/multipleCucumberReportHtml.ts $CYPRESS_ENVIRONMENT",
    "lint": "eslint . \"**/*.{js,ts}\" --ignore-path .gitignore",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.1",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "@types/cypress": "^1.1.3",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.1",
    "cucumber": "^6.0.7",
    "cypress": "^13.6.6",
    "cypress-cucumber-preprocessor": "^4.3.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
